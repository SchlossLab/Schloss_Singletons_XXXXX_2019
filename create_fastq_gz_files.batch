SAMPLE=data/human
SRR=`cut -f 26 $SAMPLE/SRP062005_info.tsv | grep "SRR"`
for tag in $SRR
do
touch $SAMPLE/${tag}_1.fastq.gz
touch $SAMPLE/${tag}_2.fastq.gz
done

