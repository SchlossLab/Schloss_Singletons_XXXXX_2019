SAMPLE=data/mice
wget -N -P $SAMPLE http://www.mothur.org/MiSeqDevelopmentData/StabilityNoMetaG.tar
SRR=`tar tvf data/mice/StabilityNoMetaG.tar | grep -v " Mo" | sed -E "s/.* ([MF].*gz)/data\/mice\/\\1/"`
for tag in $SRR
do
touch $tag
done

